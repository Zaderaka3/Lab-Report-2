//255 means full color display and 0 means color is not displayed
void setup() {
  // Set LED pins as outputs
  pinMode(9, OUTPUT); // Set red LED pin as output
  pinMode(11, OUTPUT); // Set blue LED pin as output
  pinMode(10, OUTPUT); // Set green LED pin as output
}

void loop() {
  // Using HEX, we can convert it into any RGB colors
  analogWrite(13, 24);  // Adjust red LED brightness
  analogWrite(10, 214); // Adjust green LED brightness
  analogWrite(11, 17);  // Adjust blue LED brightness
}
